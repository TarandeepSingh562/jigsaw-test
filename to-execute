
#In this step we use another module
mkdir -p mods/org.astro

/usr/java/jdk-9/bin/javac -d mods/org.astro/ src/org.astro/module-info.java src/org.astro/org/astro/World.java
/usr/java/jdk-9/bin/javac -mp mods -d mods/com.greetings/ src/com.greetings/module-info.java src/com.greetings/com/greetings/Main.java
/usr/java/jdk-9/bin/java -mp mods -m com.greetings/com.greetings.Main
